  "private": true,
  "browserslist": [
    "last 2 versions",
    "android 4",
    "opera 12"
  ],
  "scripts": {
    "build-admin:watch": "webpack --env.TAILWIND_MODE=watch --mode development --watch --progress --config admin/src/build/webpack.config.js",
    "build-admin:production": "webpack --env.NODE_ENV=production --mode production --progress --config admin/src/build/webpack.config.js",
    "build-public:watch": "webpack --env.TAILWIND_MODE=watch --mode development --watch --progress --config public/src/build/webpack.config.js",
    "build-public:production": "webpack --env.NODE_ENV=production --mode production --progress --config public/src/build/webpack.config.js",
